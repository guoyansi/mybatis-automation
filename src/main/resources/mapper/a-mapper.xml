<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.IADao">
    <select id="selectList" parameterType="mapper.ABean" resultType="mapper.ABean">
		select f_tinyint,f_smallint,f_mediumint,f_int,integer,f_bigint,f_double,f_float,f_decimal,f_numeric,f_char,f_varchar,f_year,f_tinytext,f_text,f_longtext,isDet from a
        <where>${sqlWhere}</where>
        <if test="orderBy!=null">order by ${sqlOrderBy}</if>
        <if test="sqlLimit==true">LIMIT #{sqlStartIndex},#{sqlPageSize}</if>
    </select>
    <select id="selectOne" parameterType="mapper.ABean" resultType="mapper.ABean">
		select f_tinyint,f_smallint,f_mediumint,f_int,integer,f_bigint,f_double,f_float,f_decimal,f_numeric,f_char,f_varchar,f_year,f_tinytext,f_text,f_longtext,isDet from a
        <where>${sqlWhere}</where>
    </select>
    <select id="count" parameterType="mapper.ABean" resultType="int">
		select count(*) from a
        <where>${sqlWhere}</where>
    </select>
    <update id="update" parameterType="mapper.ABean">
		update a
        <if test="SqlSet==null">
            <set>
			f_tinyint=#{f_tinyint},
			f_smallint=#{f_smallint},
			f_mediumint=#{f_mediumint},
			f_int=#{f_int},
			integer=#{integer},
			f_bigint=#{f_bigint},
			f_double=#{f_double},
			f_float=#{f_float},
			f_decimal=#{f_decimal},
			f_numeric=#{f_numeric},
			f_char=#{f_char},
			f_varchar=#{f_varchar},
			f_year=#{f_year},
			f_tinytext=#{f_tinytext},
			f_text=#{f_text},
			f_longtext=#{f_longtext},
			isDet=#{isDet},</set>
        </if>
        <if test="set!=null">
            <set>${sqlSet}</set>
        </if>
        <where>${sqlWhere}</where>
    </update>
    <insert id="insert" parameterType="mapper.ABean">
		insert a
		(f_smallintf_mediumintf_intintegerf_bigintf_doublef_floatf_decimalf_numericf_charf_varcharf_yearf_tinytextf_textf_longtextisDet)
		 values
		(#{f_smallint}#{f_mediumint}#{f_int}#{integer}#{f_bigint}#{f_double}#{f_float}#{f_decimal}#{f_numeric}#{f_char}#{f_varchar}#{f_year}#{f_tinytext}#{f_text}#{f_longtext}#{isDet})	</insert>
    <insert id="insertGetId" keyProperty="" useGeneratedKeys="true" parameterType="mapper.ABean">
		insert a
		(f_smallintf_mediumintf_intintegerf_bigintf_doublef_floatf_decimalf_numericf_charf_varcharf_yearf_tinytextf_textf_longtextisDet)
		 values
		(#{f_smallint}#{f_mediumint}#{f_int}#{integer}#{f_bigint}#{f_double}#{f_float}#{f_decimal}#{f_numeric}#{f_char}#{f_varchar}#{f_year}#{f_tinytext}#{f_text}#{f_longtext}#{isDet})	</insert>
    <delete id="delete" parameterType="mapper.ABean">
		DELETE FROM a
        <where>${sqlWhere}</where>
    </delete>
    <delete id="deleteById" parameterType="object">
		DELETE FROM a
        <where>=#{}</where>
    </delete>
</mapper>
